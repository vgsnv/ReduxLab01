{
    "compilerOptions": {
        "outDir": "./dist/",
        "sourceMap": true,
        "noImplicitAny": true,
        "module": "commonjs",
        "target": "es5",
        "jsx": "react",
        "baseUrl": "./src",
        "paths": {
            "actions": ["actions/*"],
            "models": ["models/*"],
            "components": ["components/*"],
            "reducers": ["reducers/*"]
        }
    },
    "include": [
        "./src/**/*"
    ]
}